        <rdf:type rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#OpticalSpectrometer"/>
        <om-2:alternativeLabel>High Accuracy Radial velocity Planet Searcher</om-2:alternativeLabel>
        <rdfs:label>HARPS</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Helium -->

    <owl:NamedIndividual rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Helium">
        <rdf:type rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#GasElement"/>
        <exoplanet-ontology:hasChemicalSymbol>He</exoplanet-ontology:hasChemicalSymbol>
        <rdfs:label xml:lang="en">helium</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#HotJupiter -->

    <owl:NamedIndividual rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#HotJupiter">
        <rdfs:comment xml:lang="en">A type of gas giant exoplanet that is similar in size and composition to Jupiter but orbits very close to its host star. These planets have short orbital periods and high surface temperatures due to their proximity to the star, leading to extreme heating and atmospheric conditions.</rdfs:comment>
        <rdfs:label xml:lang="en">hot Jupiter</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#HotNeptune -->

    <owl:NamedIndividual rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#HotNeptune">
        <rdfs:comment xml:lang="en">A planet that shares similarities with Neptune in terms of size and composition but orbits very close to its host star, similar to hot Jupiters. These planets have shorter orbital periods and higher temperatures compared to their counterparts in our solar system.</rdfs:comment>
        <rdfs:label xml:lang="en">hot Neptune</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#HubbleSpaceTelescope -->

    <owl:NamedIndividual rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#HubbleSpaceTelescope">
        <rdf:type rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#ObservationInstrument"/>
        <exoplanet-ontology:detects rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Infrared"/>
        <exoplanet-ontology:detects rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#VisibleLight"/>
        <exoplanet-ontology:detects rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#ultraviolet"/>
        <exoplanet-ontology:name>Hubble Space Telescope</exoplanet-ontology:name>
        <om-2:abbreviation xml:lang="en">HST</om-2:abbreviation>
        <rdfs:comment xml:lang="en">One of the most famous and successful observatories ever launched. It has revolutionized our understanding of the universe with its high-resolution images and observations across a wide range of wavelengths. Hubble has contributed to numerous discoveries, including the measurement of the Hubble constant, the identification of dark matter, and the determination of the age of the universe.</rdfs:comment>
        <rdfs:label xml:lang="en">Hubble Space Telescope</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Hydrogen -->

    <owl:NamedIndividual rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Hydrogen">
