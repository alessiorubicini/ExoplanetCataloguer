    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#MichalGillon -->

    <owl:NamedIndividual rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#MichalGillon">
        <rdf:type rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Researcher"/>
        <exoplanet-ontology:isAffiliatedWith rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#UniversityOfLige"/>
        <exoplanet-ontology:name>Michal Gillon</exoplanet-ontology:name>
        <rdfs:label>Michal Gillon</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#MicrolensingMethod -->

    <owl:NamedIndividual rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#MicrolensingMethod">
        <rdf:type rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#DetectionMethod"/>
        <exoplanet-ontology:name>Microlensing</exoplanet-ontology:name>
        <rdfs:comment xml:lang="en">A method that utilizes the phenomenon of gravitational microlensing to detect exoplanets. When a massive object, such as a star or a planet, passes in front of a more distant star, its gravitational field bends and magnifies the light from the background star. If the foreground object has a planet orbiting it, the additional gravitational influence of the planet can create subtle distortions in the magnification pattern. By carefully analyzing these distortions in the light curve of the background star, scientists can detect and characterize exoplanets.</rdfs:comment>
        <rdfs:label xml:lang="en">microlensing method</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Microwaves -->

    <owl:NamedIndividual rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Microwaves">
        <rdf:type rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#ElectromagneticSpectrum"/>
        <rdfs:comment xml:lang="en">Slightly shorter wavelengths than radio waves, microwaves are used to study thermal radiation from objects like interstellar dust clouds, and to investigate the cosmic microwave background radiation.</rdfs:comment>
        <rdfs:label xml:lang="en">microwaves</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Nitrogen -->

    <owl:NamedIndividual rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Nitrogen">
