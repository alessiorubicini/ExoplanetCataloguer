     -->

    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#detectedBy -->

    <owl:ObjectProperty rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#detectedBy">
        <owl:inverseOf rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#detects"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#ElectromagneticSpectrum"/>
        <rdfs:range rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#ObservationInstrument"/>
        <rdfs:comment xml:lang="en">Relates a electromagnetic spectrum to the observation instruments that can detect it.</rdfs:comment>
        <rdfs:label xml:lang="en">detected by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#detectedThrough -->

    <owl:ObjectProperty rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#detectedThrough">
