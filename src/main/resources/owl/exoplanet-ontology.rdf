    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#PlanetMass -->

    <owl:DatatypeProperty rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#PlanetMass">
        <rdfs:domain rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Planet"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#double">0.0</xsd:minExclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:label xml:lang="en">planet mass</rdfs:label>
        <exoplanet-ontology:hasUnitOfMeasure rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#EarthMass"/>
    </owl:DatatypeProperty>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#distanceFromEarth -->

    <owl:DatatypeProperty rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#distanceFromEarth">
