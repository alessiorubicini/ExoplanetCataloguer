        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Planet"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:comment xml:lang="en">Relates a planet to the researchers who discovered it.</rdfs:comment>
        <rdfs:label xml:lang="en">discovered by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#discoveredWith -->

    <owl:ObjectProperty rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#discoveredWith">
        <owl:inverseOf rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#usedToDiscover"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Planet"/>
        <rdfs:range rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#ObservationInstrument"/>
        <rdfs:comment xml:lang="en">Relates a planet to the detection method used to discover it.</rdfs:comment>
        <rdfs:label xml:lang="en">discovered with</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#hasAtmosphereMainlyComposedOf -->

