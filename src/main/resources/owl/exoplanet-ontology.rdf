        <rdfs:domain rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#CelestialBody"/>
        <rdfs:range rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#CelestialBody"/>
        <rdfs:comment xml:lang="en">Relates a celestial body to a satellite in its orbit.</rdfs:comment>
        <rdfs:label xml:lang="en">has satellite</rdfs:label>
    </owl:ObjectProperty>
    


