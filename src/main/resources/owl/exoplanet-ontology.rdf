        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Planet"/>
        <rdfs:range rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#DetectionMethod"/>
        <rdfs:comment xml:lang="en">Relates a planet to the detection method used to detect it.</rdfs:comment>
        <rdfs:label xml:lang="en">detected through</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#detects -->

    <owl:ObjectProperty rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#detects">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#ObservationInstrument"/>
        <rdfs:range rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#ElectromagneticSpectrum"/>
        <rdfs:comment xml:lang="en">Relates an observation instrument to the electromagnetic spectrums it can detect.</rdfs:comment>
        <rdfs:label xml:lang="en">detects</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#discoveredBy -->

    <owl:ObjectProperty rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#discoveredBy">
