        <exoplanet-ontology:hasChemicalSymbol>H</exoplanet-ontology:hasChemicalSymbol>
        <rdfs:label xml:lang="en">hydrogen</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Infrared -->

    <owl:NamedIndividual rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Infrared">
        <rdf:type rdf:resource="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#ElectromagneticSpectrum"/>
        <rdfs:comment xml:lang="en">Longer wavelengths than visible light, infrared radiation allows astronomers to observe cool objects such as protostars, dust clouds, and the heat emitted by planets, including our own Earth.</rdfs:comment>
        <rdfs:label xml:lang="en">infrared</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Iron -->

    <owl:NamedIndividual rdf:about="https://www.unicam.it/cs/alessiorubicini/exoplanet-ontology#Iron">
